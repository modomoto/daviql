.card-block
  %h4.card-title
    = fa_icon "file-excel-o"
    = query.name
    %span.pull-right
      \#
      = query.id
  %p.card-text
    = truncate(query.description, :length => 20  )
  %a.btn.btn-primary{:href => url_for(:action => 'show', :controller => "interpreter/#{query.interpreter}", :id => query.id, format: 'csv')} Download data