<div class="row-fluid">
	<div class="span3">
	  <div class="well sidebar-nav">
	    <ul class="nav nav-list">
	      <li class="nav-header">Queries:</li>
	      	<% @queries.each do |query| %>
				<li>
					<a href="<%= url_for(:action => 'show', :controller => "interpreter/#{query.interpreter}", :id => query.id) %>"><i class='<%= list_icon query.interpreter %>'></i><%= query.name %></a>
				</li>
			<% end %>
	    </ul>
	  </div><!--/.well -->

	  <div class="thumbnail">
		      <img src="http://placedog.com/300/200" alt="">
		      <div class="caption">
			      <h3>Just a random dog</h3>
			   </div>
		    </div>

	</div><!--/span-->
	<div class="span9">
	  <h1>DaViQl <small>Data Visualization of SQl</small></h1> 

	  <div class="row-fluid">
	  	<% @queries.group_by(&:context).each do |context, context_queries| %>
	  	<h3><%= context %> </h3>
		  <ul class="thumbnails">
	  		<% context_queries.each do |query| %>
			  <li class="span3">
			    <div class="thumbnail">
			      <div class="caption">
				      <h3><a href="<%= url_for(:action => 'show', :controller => "interpreter/#{query.interpreter}", :id => query.id) %>"><i class='<%= list_icon query.interpreter %>'></i> <%= query.name %></a></h3>
				      <p>
				      	<%= query.description%>
				      </p>
				      <p>
				      	<a href="<%= url_for(:action => 'show', :controller => "interpreter/#{query.interpreter}", :id => query.id) %>" class="btn ">Show data</a>				      	
				      </p>
				   </div>
			    </div>
			  	</li>
			 <% end %>
			</ul>
			<% end %>
		</div>
	  


	</div>
</div><!--/row-->